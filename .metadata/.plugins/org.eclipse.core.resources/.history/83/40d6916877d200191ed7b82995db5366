
public class Application {
	public static void main (String [] args) {
		int array [][] = new int [8][8];
		for (int i = 0; i<8; i++){
			for(int j = 0; j<8; j++) {
				array[i][j] = 0; 
			}
		}
		for (int a = 0; a<8; a++){
			for(int b = 0; b<8; b++) {
				array[a][b] = 0; 
				System.out.println(array[a][b]);
			}
		}
	
	}

}
